{
  "aws": {
    "region": "us-east-1",
    "access_key_id": null,
    "secret_access_key": null,
    "session_token": null,
    "profile_name": null
  },
  "database": {
    "table_name": "data-pipeline-table",
    "billing_mode": "PAY_PER_REQUEST",
    "read_capacity": 5,
    "write_capacity": 5
  },
  "api": {
    "openweather_api_key": null,
    "exchangerate_api_key": null,
    "github_api_key": null
  },
  "lambda": {
    "function_name": "data-pipeline-handler",
    "runtime": "python3.9",
    "timeout": 30,
    "memory_size": 128,
    "environment_variables": {
      "DYNAMODB_TABLE": "data-pipeline-table",
      "AWS_REGION": "us-east-1"
    }
  },
  "dashboard": {
    "host": "0.0.0.0",
    "port": 8501,
    "debug": false,
    "theme": "light"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_path": null
  },
  "project": {
    "name": "Cloud Data Orchestrator",
    "version": "2.0.0",
    "environment": "development",
    "debug": false
  }
}